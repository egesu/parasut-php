{
    "name": "leventcz/parasut-php",
    "description": "Parasut API v4 client for PHP",
    "keywords": ["parasut", "php", "sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Levent Çorapsız",
            "email": "l.corapsiz@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Leventcz\\Parasut\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.x-dev",
        "laravel/pint": "^1.5.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
