{
    "name": "leventcz/parasut-php",
    "description": "Parasut API v4 client for PHP",
    "keywords": [
        "parasut",
        "php",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Levent Çorapsız",
            "email": "l.corapsiz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0.0",
        "pestphp/pest-plugin-mock": "^2.0.0",
        "laravel/pint": "^1.5.0",
        "symfony/var-dumper": "^6.2",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Leventcz\\Parasut\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test:unit": "pest --colors=always"
    }
}
